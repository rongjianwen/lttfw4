{
    "name": "@smfw/web",
    "libname": "SmfwWEB",
    "version": "0.1.0",
    "main": "src/index.ts",
    "license": "UNLICENSED",
    "scripts": {
        "precommit": "lint-staged",
        "build": "npm-run-all build:types build:umd build:test",
        "build:umd": "rimraf build/umd && npm-run-all build:umd:dev build:umd:prod",
        "build:umd:dev": "rimraf build/umd/*.development*.js && cross-env NODE_ENV=development rollup -c rollup.config.js",
        "build:umd:prod": "rimraf build/umd/*.production*.js && cross-env NODE_ENV=production rollup -c rollup.config.js",
        "build:types": "rimraf types/* && tsc -d --declarationDir types --emitDeclarationOnly",
        "build:test": "rimraf build/test/* && cross-env NODE_ENV=production webpack --config ./configs/webpack.test.config.js",
        "start:test": "cross-env NODE_ENV=development webpack serve --config ./configs/webpack.test.config.js"
    },
    "devDependencies": {
        "@babel/core": "^7.16.5",
        "@babel/eslint-parser": "^7.16.5",
        "@babel/plugin-proposal-class-properties": "^7.16.5",
        "@babel/preset-env": "^7.16.5",
        "@babel/preset-react": "^7.16.5",
        "@babel/preset-typescript": "^7.16.5",
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-eslint": "^8.0.1",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^11.0.1",
        "@rollup/plugin-replace": "^3.0.0",
        "@rollup/plugin-yaml": "^3.0.0",
        "@types/node": "^17.0.4",
        "@typescript-eslint/eslint-plugin": "^5.8.0",
        "@typescript-eslint/parser": "^5.8.0",
        "babel-loader": "^8.2.3",
        "core-js": "^3.20.1",
        "cross-env": "^7.0.3",
        "eslint": "^8.5.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.3",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-react": "^7.28.0",
        "html-webpack-plugin": "^5.5.0",
        "lint-staged": "^12.1.4",
        "mini-css-extract-plugin": "^2.4.5",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.5.1",
        "process": "^0.11.10",
        "rimraf": "^3.0.2",
        "rollup": "^2.35.1",
        "rollup-plugin-cleanup": "^3.2.1",
        "rollup-plugin-external-globals": "^0.6.1",
        "rollup-plugin-json": "^4.0.0",
        "rollup-plugin-summary": "^1.3.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.29.0",
        "ts-loader": "^9.2.6",
        "typescript": "^4.5.4",
        "unzipper": "^0.10.11",
        "webpack": "^5.65.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.7.1",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "@material-ui/core": "^4.12.3",
        "@reduxjs/toolkit": "^1.6.2",
        "@types/chroma-js": "^2.1.3",
        "@types/lodash": "^4.14.177",
        "@types/material-ui": "^0.21.12",
        "@types/react": "^17.0.35",
        "@types/react-dom": "^17.0.11",
        "@types/redux-logger": "^3.0.9",
        "chroma-js": "^2.1.2",
        "lodash": "^4.17.21",
