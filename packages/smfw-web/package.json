        "react-dom": "^17.0.2",
        "react-redux": "^7.2.6",
        "react-router-dom": "^6.0.2",
        "redux": "^4.1.2",
        "redux-devtools-extension": "^2.13.9",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.4.1",
        "reduxjs-toolkit-persist": "^7.0.7"
    }
}
